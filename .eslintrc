{
  "extends": "rallycoding",
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": [".js", ".ios.js", ".android.js", ".json"],
        "alias": {
          "routes": "./src/routes",
          "components": "./src/components",
          "theme": "./src/theme",
          "utils": "./src/utils",
          "fixtures": "./src/fixtures",
          "domains": "./src/domains",
          "models": "./src/redux/models",
          "config": "./src/config",
          "services": "./src/services",
          "selectors": "./src/selectors",
          "sagas": "./src/sagas"
        }
      }
    }
  }
}
